Module Module1

    Sub Main()
        Dim Lado As Integer
        Dim Perimetro As Integer

        Console.Write("Intruduzca cuanto valen los lados del cuadrado:")
        Lado = Console.ReadLine()
        Perimetro = Lado * 4
        Console.Write(" El perimetro del cuadrado es:")
        Console.Write(Perimetro)
        Console.ReadKey()

    End Sub

End Module
